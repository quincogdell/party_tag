<div id="javascript-info" class="hidden" data-eventid=<%=@event.id%> data-hash-tags=<%= @event.hash_tags.first.tag_name %>>0</div>
<div class="nav second" id="second">
  <h2><%= @event.title %></h2>
  <ul>
    <li>
    <strong>At:</strong>
    <%= @event.location %>
    </li>
    <li>
    <strong>On:</strong>
    <%= "#{@event.date}" %>
    </li>
    <li>
    <strong>tracked tags:</strong>
    <%= @event.hash_tags.map  { |hash| "##{hash.tag_name}"}.join(", ") %>
    </li>
    <li>
    <%= link_to "edit", "/events/#{@event.id}/edit" %>
    </li>
  </ul>
</div>
