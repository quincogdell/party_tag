<div id="nav-case"></div>

  <div class="userevents">
      <% @events.each do |event| %>
        <h3><%= link_to event.title, "/events/#{event.id}" %></h3>
        <% event.scrapbooks.each do |scrapbook| %>
          <a href=<%= "/scrapbooks/#{scrapbook.id}" %>>
          <img class='preview' src=<%= scrapbook.canvas %>>
        </a>
        <% end %>

        <div class="new-scrapbook">
          <a href= <%= "/events/#{event.id}" %>>
            <img class="add-image" src="additition_sign.png">
          </a>
        </div>

    <% end %>
  </div>


